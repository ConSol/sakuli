{
  "title": "sakuli",
  "description": "Sakuli Documentation",
  "author": "The Sakuli Team",
  "extension": "pdf",
  "github": "ConSol/sakuli",
  "githubHost": "https://github.com/",
  "gitbook": "2.x.x",
  "links": {
    "home": "https://github.com/ConSol/sakuli",
    "about": "https://consol.github.io/sakuli/${sakuli.version}/index.html",
    "issues": "https://github.com/ConSol/sakuli/issues",
    "sharing": {
      "twitter": "https://twitter.com/sakuli_e2e"
    }
  },
  "plugins": [
    "edit-link@2.0.2",
    "anchors@0.7.1",
    "code_tomorrow_scheme@0.1.3"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/ConSol/sakuli/edit/${git.branch}/docs",
      "label": "Edit This Page"
    }
  },
  "pdf": {
    "toc": true,
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 36,
      "bottom": 36
    }
  }
}