{# This template renders the performance data for a specific test step. #}
{% if (not empty(testCaseIndex)) %}
    s_{{ testCaseIndex }}_{{ testStepIndex }}_{{testStep.id}}={{getOutputDuration(testStep)}};
{% else %}
    s_{{ testStepIndex }}_{{testStep.id}}={{getOutputDuration(testStep)}};
{% endif %}
{% set testStepWarningTime = testStep.warningTime %}
{% if (testStepWarningTime > 0) %}
    {{testStepWarningTime}}
{% else %}
{% endif %};;;
