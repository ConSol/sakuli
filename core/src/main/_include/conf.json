{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./"],
        "includePattern": ".*\.js",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "markdown": {
        "parser": "gfm"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/verboseOutput"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}